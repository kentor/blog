{
  "name": "kentor.github.io",
  "version": "1.0.0",
  "description": "blog",
  "main": "lib/index.js",
  "scripts": {
    "assets:build": "gulp build",
    "assets:watch": "gulp watch",
    "build": "npm-run-all html:build assets:build",
    "clean": "rimraf public && mkdir -p public",
    "deploy": "npm-run-all clean build deploy-public",
    "deploy-public": "node lib/deploy.js",
    "html:build": "node lib/build.js",
    "html:watch": "node .",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "start": "npm-run-all clean html:build --parallel assets:watch html:watch lint:watch webserver",
    "webserver": "browser-sync start -s public --files 'public/**/*' --no-notify --no-open"
  },
  "author": "Kenneth Chung",
  "homepage": "http://kentor.me/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kentor/kentor.github.io.git"
  },
  "devDependencies": {
    "babel-preset-react": "6.5.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.12.10",
    "classnames": "2.2.5",
    "colors": "1.1.2",
    "del": "2.2.0",
    "dependency-graph": "0.5.0",
    "eslint": "2.10.2",
    "eslint-config-kentor": "2.0.0",
    "eslint-plugin-react": "5.1.1",
    "eslint-watch": "2.1.11",
    "farmhash": "1.1.1",
    "front-matter": "2.0.8",
    "fs-promise": "0.5.0",
    "gh-pages": "0.11.0",
    "gulp": "3.9.1",
    "gulp-cssnano": "2.1.2",
    "gulp-postcss": "6.1.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uncss": "1.0.5",
    "immutable": "3.8.1",
    "marky-markdown": "7.0.0",
    "moment": "2.13.0",
    "npm-run-all": "2.1.1",
    "postcss-assets": "4.1.0",
    "postcss-cssnext": "2.5.2",
    "postcss-import": "8.1.2",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "rimraf": "2.5.2",
    "tachyons": "4.0.0-beta.28",
    "touch": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  }
}
