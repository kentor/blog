{
  "name": "kentor.me",
  "version": "1.0.0",
  "description": "blog",
  "main": "lib/index.js",
  "scripts": {
    "assets:build": "gulp build",
    "assets:watch": "gulp watch",
    "build": "NODE_ENV=production npm-run-all clean html:build -p assets:build js:build -s rev",
    "clean": "rimraf public && mkdirp public",
    "deploy": "firebase deploy --project blog-b5600",
    "gulp": "gulp",
    "html:build": "babel-node lib/index.js",
    "html:watch": "babel-node lib/index.js --watch",
    "js:build": "browserify src/js/index.js -p bundle-collapser/plugin -p dep-case-verify -g loose-envify -g unreachable-branch-transform | uglifyjs -c -m -o public/bundle.js",
    "js:watch": "watchify src/js/index.js -p dep-case-verify -o public/bundle.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "rev": "gulp rev",
    "serve": "browser-sync start -s public --files 'public/**/*' --no-notify --no-open --reload-debounce 1",
    "start": "npm-run-all clean -p assets:watch html:watch lint:watch js:watch serve"
  },
  "author": "Kenneth Chung",
  "homepage": "http://kentor.me/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kentor/kentor.me.git"
  },
  "dependencies": {
    "algoliasearch": "3.22.1",
    "babel-cli": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-promise-to-bluebird": "1.1.1",
    "babel-preset-env": "1.4.0",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "bluebird": "3.5.0",
    "browser-sync": "2.18.8",
    "browserify": "14.3.0",
    "bundle-collapser": "1.2.1",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "chokidar": "1.6.1",
    "classnames": "2.2.5",
    "commander": "2.9.0",
    "debounce-promise": "3.0.1",
    "debug": "2.6.6",
    "dep-case-verify": "2.1.0",
    "dependency-graph": "0.5.0",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-kentor": "3.19.2",
    "eslint-plugin-react": "7.0.0",
    "eslint-watch": "3.1.0",
    "firebase-tools": "3.7.0",
    "front-matter": "2.1.2",
    "fs-extra": "3.0.1",
    "gulp": "3.9.1",
    "gulp-cssnano": "2.1.2",
    "gulp-newer": "1.3.0",
    "gulp-postcss": "7.0.0",
    "gulp-rev-all": "0.9.7",
    "gulp-sourcemaps": "2.6.0",
    "gulp-uncss": "1.0.6",
    "invalidate-module": "0.0.1",
    "lodash": "4.17.4",
    "loose-envify": "1.3.1",
    "loud-rejection": "1.6.0",
    "marky-markdown": "9.0.3",
    "mkdirp": "0.5.1",
    "moment": "2.18.1",
    "npm-run-all": "4.0.2",
    "object-path": "0.11.4",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "reuse-promise": "1.1.0",
    "rimraf": "2.6.1",
    "rss": "1.2.2",
    "tachyons": "4.7.1",
    "uglify-js": "2.8.23",
    "unreachable-branch-transform": "0.5.1",
    "watchify": "3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
