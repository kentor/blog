{
  "name": "kentor.me",
  "version": "1.0.0",
  "description": "blog",
  "main": "lib/index.js",
  "scripts": {
    "assets:build": "gulp build",
    "assets:watch": "gulp watch",
    "build": "npm-run-all clean html:build assets:build rev",
    "clean": "rimraf public && mkdirp public",
    "deploy": "firebase deploy --project blog-b5600",
    "gulp": "gulp",
    "html:build": "babel-node lib/index.js",
    "html:watch": "babel-node lib/index.js --watch",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "rev": "gulp rev",
    "server": "browser-sync start -s public --files 'public/**/*' --no-notify --no-open --reload-debounce 1",
    "start": "npm-run-all clean --parallel assets:watch html:watch lint:watch server"
  },
  "author": "Kenneth Chung",
  "homepage": "http://kentor.me/",
  "repository": {
