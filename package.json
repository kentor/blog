{
  "name": "kentor.me",
  "version": "1.0.0",
  "description": "blog",
  "main": "lib/index.js",
  "scripts": {
    "assets:build": "gulp build",
    "assets:watch": "gulp watch",
    "build": "NODE_ENV=production npm-run-all clean html:build -p assets:build js:build -s rev",
    "clean": "rimraf public && mkdirp public",
    "deploy": "npm run build && firebase deploy --project blog-b5600",
    "gulp": "gulp",
    "html:build": "./node lib/scripts/build.js",
    "html:watch": "./node lib/scripts/watch.js",
    "js:build": "browserify assets/js/index.js -p bundle-collapser/plugin -p dep-case-verify -g loose-envify -g unreachable-branch-transform | uglifyjs -c -m -o public/bundle.js",
    "js:watch": "watchify assets/js/index.js -p dep-case-verify -o public/bundle.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "rev": "gulp rev",
    "serve": "browser-sync start -s public --files 'public/**/*' --no-ghost-mode --no-notify --no-open --no-ui --reload-debounce 1",
    "start": "npm-run-all clean -p assets:watch html:watch lint:watch js:watch serve"
  },
  "author": "Kenneth Chung",
  "homepage": "http://kentor.me/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kentor/kentor.me.git"
  },
  "dependencies": {
    "babel-eslint": "8.0.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babelify": "7.3.0",
    "browser-sync": "2.18.13",
    "browserify": "14.4.0",
    "bundle-collapser": "1.3.0",
    "chalk": "2.1.0",
    "cheerio": "0.22.0",
    "chokidar": "1.7.0",
    "classnames": "2.2.5",
    "dep-case-verify": "2.1.0",
    "dependency-graph": "0.5.0",
    "dotenv": "4.0.0",
    "eslint": "4.7.1",
    "eslint-config-kentor": "5.0.0",
    "eslint-watch": "3.1.2",
    "firebase-tools": "3.12.0",
    "front-matter": "2.2.0",
    "fs-extra": "4.0.2",
    "gulp": "3.9.1",
    "gulp-cssnano": "2.1.2",
    "gulp-newer": "1.3.0",
    "gulp-postcss": "7.0.0",
    "gulp-rev-all": "0.9.7",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uncss": "1.0.6",
    "invalidate-module": "1.0.0",
    "loose-envify": "1.3.1",
    "loud-rejection": "1.6.0",
    "marky-markdown": "11.2.0",
    "mkdirp": "0.5.1",
    "moment": "2.18.1",
    "npm-run-all": "4.1.1",
    "object-path": "0.11.4",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "11.0.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "reuse-promise": "1.1.0",
    "rimraf": "2.6.2",
    "rss": "1.2.2",
    "tachyons": "4.8.1",
    "tiny-ssg": "1.1.0",
    "uglify-js": "3.1.1",
    "unreachable-branch-transform": "0.5.1",
    "watchify": "3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
